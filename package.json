		"eslint-plugin-vue": "^6.0.1",
		"file-loader": "^5.0.2",
		"mini-css-extract-plugin": "^0.8.0",
		"postcss": "^8.2.10",
		"uglifyjs-webpack-plugin": "^2.2.0",
		"vue-loader": "^15.7.2",
